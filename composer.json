{
    "name": "malek83/polish-vat-payer",
    "description": "Library using Polish Ministry of Finance WebService to validate if given VAT Number is registered as VAT Tax Payer in Poland",
    "license": "MIT",
    "version": "3.0.0",
    "authors": [
        {
            "name": "Michał Małecki",
            "homepage": "http://negen.pl"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "ext-soap": "*",
        "psr/simple-cache": "~1.0",
        "psr/log": "~1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0 || ^5.0",
        "squizlabs/php_codesniffer": "3.*",
        "desarrolla2/cache": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Malek83\\PolishVatPayer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Malek83\\PolishVatPayer\\Test\\": "tests/"
        }
    }
}
